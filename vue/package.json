    "rules": {
      "no-unexpected-multiline": 0,
      "no-irregular-whitespace": 0,
      "no-unused-vars": 0,
      "indent": 0,
      "no-tabs": 0,
      "no-mixed-spaces-and-tabs": 0,
      "vue/no-unused-components": "off",
      "vue/multi-word-component-names": "off",
      "vue/no-useless-template-attributes": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
